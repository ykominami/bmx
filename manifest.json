{
  "name": "Bmx72MV3",
  "description": "Powered-Bookmark7MV3",
  "version": "2.3",
  "manifest_version": 3,
  "permissions": [
      "bookmarks",
      "activeTab",
      "storage",
      "tabs"
  ],
  "host_permissions": [],
  "action": {
      "default_title": "Bmx7MV3",
      "default_icon": "images/19x19.png",
      "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}